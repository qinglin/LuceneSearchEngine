package src.ir.assignments.three;
// having this class saving the status of sub-domains 
public class Subdomain {
	private String URL;
	private int number;
	public Subdomain(String URL, int number)
	{
		this.number = number;
		this.URL = URL;
	}
	public String getURL()
	{
		return URL;
	}
	public int getNumber()
	{
		return number;
	}
	public void incrementNumOfURL()
	{
		number++;
	}
	public void incrementNumOfURL(int num)
	{
		number+=num;
	}

}
